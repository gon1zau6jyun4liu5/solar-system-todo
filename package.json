{
  "name": "solar-system-todo",
  "version": "0.2.2",
  "private": true,
  "dependencies": {
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^0.0.0",
    "three": "^0.178.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=false react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "cross-env NODE_ENV=development GENERATE_SOURCEMAP=true react-scripts start",
    "build:dev": "cross-env NODE_ENV=development react-scripts build",
    "audit-fix": "npm audit fix",
    "security-check": "npm audit",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "fresh-install": "npm run reinstall && npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "@mediapipe/tasks-vision": {
      "webpack": "^5.64.4"
    }
  },
  "resolutions": {
    "react-scripts": "5.0.1"
  }
